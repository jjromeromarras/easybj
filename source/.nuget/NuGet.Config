<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <add key="gijistsw09" value="http://gijitsw09/nugetpackagemanager/nuget/" protocolVersion="2" />
    <add key="nexus" value="https://nexusrepo.mecalux.com/repository/nuget-proxy/" protocolVersion="2" />
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
  </packageSources>
  <config>
    <!--
    Used to specify the default location to expand packages.
    See: NuGet.exe help install
    See: NuGet.exe help update
    -->
    <add key="repositorypath" value="..\..\lib\nuget\Packages" />
    <add key="dependencyVersion" value="Highest" />
  </config>
  <solution>
    <add key="disableSourceControlIntegration" value="true" />
  </solution>
</configuration>